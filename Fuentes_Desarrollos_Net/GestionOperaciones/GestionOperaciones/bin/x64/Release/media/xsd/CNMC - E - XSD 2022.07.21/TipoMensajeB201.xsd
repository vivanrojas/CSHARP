			<xs:element name="Motivo" type="MotivoBajaB2"/>
			<xs:element name="FechaActivacion" type="xs:date"/>
		</xs:sequence>
	</xs:complexType>
	<!-- Datos del contrato de la baja -->
	<xs:complexType name="ContratoNotificacionBajaATRB201">
